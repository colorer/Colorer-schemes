<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE custom [
    <!ENTITY hrc SYSTEM "entity.j2ee.hrc">
    <!ENTITY cst SYSTEM "entity.application.xml">
]>
<custom xmlns="uri:colorer:custom">

<custom-type targetNamespace='http://java.sun.com/j2ee/dtds/application_1_2.dtd'>
    <prefix>application_1_2</prefix>
    <empty-prefix/>
</custom-type>

<custom-type targetNamespace='http://java.sun.com/dtd/application_1_3.dtd'>
    <prefix>application_1_3</prefix>
    <empty-prefix/>
</custom-type>

<custom-type targetNamespace="http://java.sun.com/xml/ns/j2ee">
    <prefix>j2ee</prefix>
    &cst;
    <type xmlns="http://colorer.sf.net/2003/hrc">
        &hrc;
    </type>
</custom-type>

<custom-type targetNamespace="http://java.sun.com/xml/ns/javaee">
    <prefix>javaee</prefix>
    &cst;
    <type xmlns="http://colorer.sf.net/2003/hrc">
        &hrc;
        <scheme name="http-methodType-content">
            <!--
                Warning: RE pattern used. Possible compatibility faults
                Original RE: [!-~-[\(\)<>@,;:"/\[\]?=\{\}\\\p{Z}]]+-->
            <regexp region="Enumeration" priority="low" match="/[!-\~-[\(\)&lt;&gt;@,;:&#34;\/\[\]?\=\{\}\\\s]]+/"/>
        </scheme>
    </type>
</custom-type>

</custom>