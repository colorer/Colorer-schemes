<?xml version="1.0"?>

<patterns>

  <re-patterns>
    <match token="\\"     subst="\\"/>
    <match token="/"      subst="\/"/>
    <match token="%"      subst="\%"/>
    <match token="#"      subst="\#"/>
    <match token="~"      subst="\~"/>
    <match token="="      subst="\="/>
    <match token="\i"     subst="[%xml:NCNameStartChar;]"/>
    <match token="\I"     subst="[^%xml:NCNameStartChar;]"/>
    <match token="\c"     subst="[%xml:NCNameChar;]"/>
    <match token="\C"     subst="[^%xml:NCNameChar;]"/>

    <match token="\p{Lu}" subst="{Lu}"/>
    <match token="\P{Lu}" subst="[^{Lu}]"/>
    <match token="\p{L}"  subst="{L}"/>
    <match token="\P{L}"  subst="[^{L}]"/>

    <match token="\p{Ll}" subst="{Ll}"/>
    <match token="\P{Ll}" subst="[^{Ll}]"/>

    <match token="\p{Lt}" subst="{Lt}"/>
    <match token="\P{Lt}" subst="[^{Lt}]"/>

    <match token="\p{Lm}" subst="{Lm}"/>
    <match token="\P{Lm}" subst="[^{Lm}]"/>

    <match token="\p{Lo}" subst="{Lo}"/>
    <match token="\P{Lo}" subst="[^{Lo}]"/>

    <match token="\p{Mn}" subst="{Mn}"/>
    <match token="\P{Mn}" subst="[^{Mn}]"/>
    <match token="\p{M}"  subst="{M}"/>
    <match token="\P{M}"  subst="[^{M}]"/>

    <match token="\p{Mc}" subst="{Mc}"/>
    <match token="\P{Mc}" subst="[^{Mc}]"/>

    <match token="\p{Me}" subst="{Me}"/>
    <match token="\P{Me}" subst="[^{Me}]"/>

    <match token="\p{Nd}" subst="{Nd}"/>
    <match token="\P{Nd}" subst="[^{Nd}]"/>
    <match token="\p{N}"  subst="{N}"/>
    <match token="\P{N}"  subst="[^{N}]"/>

    <match token="\p{Nl}" subst="{Nl}"/>
    <match token="\P{Nl}" subst="[^{Nl}]"/>

    <match token="\p{No}" subst="{No}"/>
    <match token="\P{No}" subst="[^{No}]"/>

    <match token="\p{Pc}" subst="{Pc}"/>
    <match token="\P{Pc}" subst="[^{Pc}]"/>
    <match token="\p{P}"  subst="{P}"/>
    <match token="\P{P}"  subst="[^{P}]"/>

    <match token="\p{Pd}" subst="{Pd}"/>
    <match token="\P{Pd}" subst="[^{Pd}]"/>

    <match token="\p{Ps}" subst="{Ps}"/>
    <match token="\P{Ps}" subst="[^{Ps}]"/>

    <match token="\p{Pe}" subst="{Pe}"/>
    <match token="\P{Pe}" subst="[^{Pe}]"/>

    <match token="\p{Pi}" subst="{Pi}"/>
    <match token="\P{Pi}" subst="[^{Pi}]"/>

    <match token="\p{Pf}" subst="{Pf}"/>
    <match token="\P{Pf}" subst="[^{Pf}]"/>

    <match token="\p{Po}" subst="{Po}"/>
    <match token="\P{Po}" subst="[^{Po}]"/>

    <match token="\p{Sm}" subst="{Sm}"/>
    <match token="\P{Sm}" subst="[^{Sm}]"/>
    <match token="\p{S}"  subst="{S}"/>
    <match token="\P{S}"  subst="[^{S}]"/>

    <match token="\p{Sc}" subst="{Sc}"/>
    <match token="\P{Sc}" subst="[^{Sc}]"/>

    <match token="\p{Sk}" subst="{Sk}"/>
    <match token="\P{Sk}" subst="[^{Sk}]"/>

    <match token="\p{So}" subst="{So}"/>
    <match token="\P{So}" subst="[^{So}]"/>

    <match token="\p{Zs}" subst="{Zs}"/>
    <match token="\P{Zs}" subst="[^{Zs}]"/>
    <match token="\p{Z}"  subst="{Z}"/>
    <match token="\P{Z}"  subst="[^{Z}]"/>

    <match token="\p{Zl}" subst="{Zl}"/>
    <match token="\P{Zl}" subst="[^{Zl}]"/>

    <match token="\p{Zp}" subst="{Zp}"/>
    <match token="\P{Zp}" subst="[^{Zp}]"/>

    <match token="\p{Cc}" subst="{Cc}"/>
    <match token="\P{Cc}" subst="[^{Cc}]"/>
    <match token="\p{C}"  subst="{C}"/>
    <match token="\P{C}"  subst="[^{C}]"/>

    <match token="\p{Cf}" subst="{Cf}"/>
    <match token="\P{Cf}" subst="[^{Cf}]"/>

    <!-- surrogates
    <match token="\p{Cs}" subst="{Cs}"/>
    <match token="\P{Cs}" subst="[^{Cs}]"/> -->

    <match token="\p{Co}" subst="{Co}"/>
    <match token="\P{Co}" subst="[^{Co}]"/>

    <match token="\p{Cn}" subst="{Cn}"/>
    <match token="\P{Cn}" subst="[^{Cn}]"/>

  </re-patterns>

  <enum-patterns>
    <match token="\" subst="\\"/>
    <match token="%" subst="\%"/>
    <match token="*" subst="\*"/>
    <match token="/" subst="\/"/>
    <match token="+" subst="\+"/>
    <match token="?" subst="\?"/>
    <match token="{" subst="\{"/>
    <match token="}" subst="\}"/>
    <match token="(" subst="\("/>
    <match token=")" subst="\)"/>
    <match token="[" subst="\["/>
    <match token="]" subst="\]"/>
    <match token="$" subst="\$"/>
    <match token="^" subst="\^"/>
    <match token="|" subst="\|"/>
    <match token="." subst="\."/>
  </enum-patterns>

</patterns>
